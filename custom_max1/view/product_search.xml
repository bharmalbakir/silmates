<odoo>
    <data>
        <record id="view_product_search_inherit" model="ir.ui.view">
            <field name="name">product.search.inherit</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_search_view"/>
            <field name="arch" type="xml">
                <xpath expr="//search" position="inside">
                    <field name="part_number" string="Part Number"/>
                </xpath>
                <xpath expr="//search" position="inside">
                    <field name="tolerance" string="Tolerance"/>
                </xpath>
                <xpath expr="//search" position="inside">
                    <field name="watts" string="Watts"/>
                </xpath>
                <xpath expr="//search" position="inside">
                    <field name="temp_coefficient" string="Temperature Coefficient"/>
                </xpath>
                <xpath expr="//search" position="inside">
                    <field name="current_rating" string="Current rating"/>
                </xpath>
                <xpath expr="//search" position="inside">
                    <field name="value" string="Value"/>
                </xpath>
                <xpath expr="//search" position="inside">
                    <field name="package" string="Package"/>
                </xpath>
                <xpath expr="//search" position="inside">
                    <field name="manufacturer" string="Manufacturer"/>
                </xpath>

            </field>
        </record>
    </data>
</odoo>